{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "eBay MCP Server",
  "url": "https://ebaymcp.com",
  "colors": {
    "primary": "#FF6B6B",
    "light": "#FFE066",
    "dark": "#4DA8FF"
  },
  "favicon": "/favicon.svg",
  "modeToggle": {
    "default": "dark",
    "isHidden": false
  },
  "stylesheet": "/styles.css",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "installation",
              "configuration"
            ]
          },
          {
            "group": "Authentication",
