    {
      "source": "/docs/:path*",
      "destination": "/:path*"
    }
  ],
  "search": {
    "prompt": "Search eBay MCP documentation..."
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "backgroundImage": "/images/background-gradient.svg",
  "topbarLinks": [
