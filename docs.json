    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://ebaymcp.com/quickstart"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
